{
    "name": "wordless-backend-emote",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "dev offline": "serverless offline --stage offline --region us-west-2",
        "dev development env": "serverless deploy --stage dev --region us-west-2",
        "dynamodb install": "serverless dynamodb install",
        "dynamodb start": "serverless dynamodb start --stage offline",
        "test": "jest --silent",
        "test:coverage": "jest --coverage",
        "test:console": "jest"
    },
    "dependencies": {
        "@aws-sdk/client-apigatewaymanagementapi": "^3.806.0",
        "@aws-sdk/client-dynamodb": "^3.585.0",
        "@aws-sdk/client-lambda": "^3.840.0",
        "@aws-sdk/lib-dynamodb": "^3.585.0",
        "@aws-sdk/util-stream": "^3.370.0",
        "jwt-decode": "^4.0.0",
        "serverless-http": "^3.2.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "aws-sdk-client-mock": "^4.1.0",
        "aws-sdk-client-mock-jest": "^4.1.0",
        "dayjs": "^1.11.13",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "guid-typescript": "^1.0.9",
        "jest": "^29.7.0",
        "mockdate": "^3.0.5",
        "prettier": "^3.4.2",
        "serverless-dynamodb": "^0.2.56",
        "serverless-mysql": "^2.0.0",
        "serverless-offline": "^14.4.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.2"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.ts"
        ],
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "preset": "ts-jest",
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/$1"
        }
    }
}
